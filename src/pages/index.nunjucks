<!-- index.nunjucks -->
{% extends "layout.nunjucks" %}

	


{% block content %}
	{% import 'macros/navigation.nunjucks' as nav %}
		<!-- Creating the navigation with activePage = 'home' -->
		{{nav.active('home')}}


	  
	 


	<main id="container" class="container">
		{% for field in fields %}
		 	{% if  field.id == 'video-background' %}
		 		{% include "partials/video.nunjucks" %}
		 	{% endif %}
		 	{% if  field.id == 'featured-project' %}
		 		{% include "partials/featured-project.nunjucks" %}
		 	{% endif %}
		 	{% if  field.id == 'hero-text' %}
		 		{% include "partials/hero-text.nunjucks" %}
		 	{% endif %}
		{% endfor %}
	</main>

	{% include "partials/footer.nunjucks" %}
{% endblock %}

